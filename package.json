{
  "name": "tts-playground",
  "version": "0.0.1",
  "author": "Aman Kumar <2000.aman.sinha@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:onlyoneaman/tts-playground.git"
  },
  "scripts": {
    "frontend-install": "cd frontend && npm install",
    "backend-install": "cd backend && npm install",
    "install-all": "npm run frontend-install && npm run backend-install",
    "start-frontend": "cd frontend && npm run dev",
    "start-backend": "cd backend && npm run start",
    "dev": "concurrently \"npm run start-frontend\" \"npm run start-backend\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
